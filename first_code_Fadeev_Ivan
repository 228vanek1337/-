
&НаСервере
Процедура ЗагрузкаДанныхНаСервере()
	
	 Таблица = Новый ТабличныйДокумент;
	 Таблица.Прочитать("D:\Данные.xlsx");
	 Строк = Таблица.ВысотаТаблицы;
	 Для Строка = 2 По Строк Цикл
		    ЭлементГород = Справочники.Города.СоздатьЭлемент();
			ЭлементГород.Наименование =	Таблица.Область(Строка,2).Текст;
			ЭлементГород.Население =	Таблица.Область(Строка,3).Текст;
			ЭлементГород.Записать();
	КонецЦикла;	 
	Сообщить("Данные загружены");	
	
КонецПроцедуры

&НаКлиенте
Процедура ЗагрузкаДанных(Команда)
	ЗагрузкаДанныхНаСервере();
КонецПроцедуры
